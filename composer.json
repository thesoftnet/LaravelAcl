{
  "name": "thesoftnet/laravel-acl",
  "description": "Laravel authentication and ACL package with a cool admin panel",
  "license": "MIT",
  "authors": [
    {
      "name": "Anwar",
      "email": "thesoftnet4u@gmail.com"
    }
  ],
  "minimum-stability": "dev",
  "repositories": [
    {
      "type":"git",
      "url": "https://github.com/thesoftnet/authentication-sentry"
    }
  ],
  "require": {
    "thesoftnet/authentication-sentry": "^1.0",
    "laravel/framework": "5.5.0-5.7.10",
    "laravelcollective/html": "5.5.0-5.7.10",
    "intervention/image": "2.*",
    "gregwar/captcha": "1.1.1"
  },

  "require-dev": {
    "phpunit/phpunit": "~5.7",
    "mockery/mockery": "0.9.*",
    "fzaninotto/faker": "~1.4",
    "sebastian/exporter": "^2.0"
  },
  "autoload": {
    "classmap": [
      "database"
    ],
    "psr-4": {
     "TheSoftNet\\LaravelAcl\\": "app/"
    }
  },
  "extra": {
    "component": "package",
    "frameworks": ["Laravel 5"],
    "laravel": {
      "providers": [
        "TheSoftNet\\LaravelAcl\\Authentication\\AuthenticationServiceProvider"
      ]
    }
  }
}
